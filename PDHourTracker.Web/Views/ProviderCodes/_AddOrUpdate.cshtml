@model PDHourTracker.Web.Models.ProviderCodes.ProviderCodeViewModel

<div class="row">
    <div class="col-md-12">
        <h1>@ViewData["Title"]</h1>
        <p class="lead">Add new provider code.</p>
    </div>
</div>
<partial name="_ProviderCodeForm" model="@Model" view-data="@ViewData" />
<div class="row">
    <div class="col-md-12">
        <p class="mt-4">
            <a asp-controller="ProviderCodes" asp-action="Index">Cancel and go back to provider codes</a>
        </p>
    </div>
</div>

@section scripts{
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}