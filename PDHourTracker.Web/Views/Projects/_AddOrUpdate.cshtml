@model PDHourTracker.Web.Models.Projects.ProjectViewModel

<div class="row">
    <div class="col-md-12">
        <h1>@ViewData["Title"]</h1>
    </div>
</div>
<partial name="_ProjectForm" model="@Model" view-data="@ViewData" />
<div class="row">
    <div class="col-md-12">
        <p class="mt-4">
            <a asp-controller="Projects" asp-action="Index">Cancel and go back to projects</a>
        </p>
    </div>
</div>

@section scripts{
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/js/form-helpers.js"></script>
    <script>
        $(function () {
            setupInputTrims('ProjectName');
        });
    </script>
}