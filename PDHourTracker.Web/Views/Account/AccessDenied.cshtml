@{ 
    ViewData["Title"] = "Access Denied";

    // Create link to requested resource
    var http = (bool)ViewData["IsHttps"] ? "https://" : "http://";
    var host = ViewData["Host"];
    var url = http + host + ViewData["ReturnUrl"];
}

<div class="row">
    <div class="col-md-12">
        <h1 class="text-center">@ViewData["Title"]</h1>
        <p>
            You do not have access to <b>@url</b>.
        </p>
        <p>
            If you feel that this is an error please contact support.
        </p>
        host: @ViewData["Host"]
        is https: @ViewData["IsHttps"]
    </div>
</div>